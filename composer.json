{
    "name": "liritopay/php-simple-mercadopago-sdk",
    "description": "A simple implementation of Mercado Pago API for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LiritoPay\\PhpSimpleMercadoPagoSdk\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiritoPay\\PhpSimpleMercadoPagoSdk\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Fernando Lira",
            "email": "local@fernand.us"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/log": "^1.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
